<%= form_for @changeset, Routes.topic_path(@conn, :update, @topic), fn f-> %>
    <div class="form-group">
        <%= text_input f, :tittle, placeholder: "Tittle", class: "form-control" %>
        <%= error_tag f, :tittle %>
    </div>

    <%= submit "Save Topic", class: "btn btn-primary" %>
<% end %>
